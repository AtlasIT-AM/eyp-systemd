[Install]

<% if defined?(@wantedby) -%>
  <%- if @wantedby.any? -%>
WantedBy=<%= @wantedby.join(' ') %>
  <%- end -%>
<% end -%>
<% if @requiredby.any? -%>
RequiredBy=<%= @requiredby.join(' ') %>
<% end -%>
<% if @service_alias.any? -%>
Alias=<%= @service_alias.join(' ') %>
<% end -%>
<% if defined?(@default_instance) -%>
DefaultInstance=<%= @default_instance %>
<% end -%>
<% if @also.any? -%>
Also=<%= @also.join(' ') %>
<% end -%>
